      "id": 140
    },
    "characters": [
      {
        "name": "M950A",
        "face": 0
      },
      {
        "name": "DJMAXSUEE",
        "face": 0
      }
    ],
    "speaker": {
      "index": 0,
      "name": "M950A"
    },
    "text": "그건…… 나도 모르겠어……"

    "character": [
      {
        "name": "SV98",
        "face": 0
      },
      {
        "name": "SVD",
        "face": 0
      }
    ],
    "speaker": {
      "index": 1,
      "name": "SVD"
    },
    "text": "그래서, 무슨 준비인데?"

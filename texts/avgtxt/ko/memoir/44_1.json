      "id": 82
    },
    "character": [
      {
        "name": "SV98",
        "face": 0
      },
      {
        "name": "SVD",
        "face": 0
      }
    ],
    "speaker": {
      "index": 0,
      "name": "SV98"
    },
    "text": "그래도 준비를 해야 해."
